<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
    <enterprise-beans>
        <session>
            <ejb-name>shenbao</ejb-name>
            <home>com.ttsoft.bjtax.shenbao.ejb.ShenbaoHome</home>
            <remote>com.ttsoft.bjtax.shenbao.ejb.Shenbao</remote>
            <ejb-class>com.ttsoft.bjtax.shenbao.ejb.ShenbaoBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>InitialStartup</display-name>
            <ejb-name>InitialStartup</ejb-name>
            <home>com.ttsoft.bjtax.shenbao.ejb.InitialStartupHome</home>
            <remote>com.ttsoft.bjtax.shenbao.ejb.InitialStartup</remote>
            <ejb-class>com.ttsoft.bjtax.shenbao.ejb.InitialStartupBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
        <session>
            <display-name>ServiceEJB</display-name>
            <ejb-name>ServiceEJB</ejb-name>
            <home>com.ttsoft.bjtax.shenbao.ejb.ServiceEJBHome</home>
            <remote>com.ttsoft.bjtax.shenbao.ejb.Service</remote>
            <ejb-class>com.ttsoft.bjtax.shenbao.ejb.ServiceEJB</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
        </session>
    </enterprise-beans>
    <assembly-descriptor>
        <container-transaction>
            <method>
                <ejb-name>InitialStartup</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <description />
                <ejb-name>shenbao</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>Required</trans-attribute>
        </container-transaction>
        <container-transaction>
            <method>
                <ejb-name>ServiceEJB</ejb-name>
                <method-name>*</method-name>
            </method>
            <trans-attribute>RequiresNew</trans-attribute>
        </container-transaction>
    </assembly-descriptor>
</ejb-jar>