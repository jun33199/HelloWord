<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE weblogic-ejb-jar PUBLIC '-//BEA Systems, Inc.//DTD WebLogic 7.0.0 EJB//EN' 'http://www.bea.com/servers/wls700/dtd/weblogic-ejb-jar.dtd'>
<weblogic-ejb-jar>
    <weblogic-enterprise-bean>
        <ejb-name>SbzlEJB</ejb-name>
        <jndi-name>SbzlEJB</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>ZhsbEJB</ejb-name>
        <transaction-descriptor>
			<trans-timeout-seconds>1800</trans-timeout-seconds>
		</transaction-descriptor>        
        <jndi-name>ZhsbEJB</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>SmOrmapInitStartUp</ejb-name>
        <stateless-session-descriptor>
            <pool>
                <max-beans-in-free-pool>1</max-beans-in-free-pool>
                <initial-beans-in-free-pool>1</initial-beans-in-free-pool>
            </pool>
        </stateless-session-descriptor>
        <jndi-name>SmOrmapInitStartUp</jndi-name>
    </weblogic-enterprise-bean>
    <weblogic-enterprise-bean>
        <ejb-name>ZqrlEJB</ejb-name>
        <transaction-descriptor>
			<trans-timeout-seconds>1800</trans-timeout-seconds>
		</transaction-descriptor>
        <jndi-name>ZqrlEJB</jndi-name>
    </weblogic-enterprise-bean>

</weblogic-ejb-jar>
